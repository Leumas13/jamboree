<odoo>
  <data>
    <record id="stmg_jamboree.jugador_form" model="ir.ui.view">
      <field name="name">stmg_jamboree.jugador.form</field>
      <field name="model">stmg_jamboree.jugador</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <field name="name" />
              <field name="nombre" />
              <field name="apellidos" />
              <field name="nacimiento" />
              <field name="equipo" />
              <field name="objetivos" />
              <field name="genero" />
              <field name="posicion" />
              <field name="lateralidad" />
              <field name="categoria" />
              <field name="dni" />
              <field name="bono" />
              <field name="sesiones" />
              <field name="fotografia" />
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="stmg_jamboree.jugador_tree" model="ir.ui.view">
      <field name="name">stmg_jamboree.jugador.tree</field>
      <field name="model">stmg_jamboree.jugador</field>
      <field name="arch" type="xml">
        <tree string="">
          <field name="name" />
              <field name="nombre" />
              <field name="apellidos" />
              <field name="nacimiento" />
              <field name="equipo" />
              <field name="objetivos" />
              <field name="genero" />
              <field name="posicion" />
              <field name="lateralidad" />
              <field name="categoria" />
              <field name="dni" />
              <field name="bono" />
              <field name="sesiones" />
        </tree>
      </field>
    </record>
  </data>
</odoo>
